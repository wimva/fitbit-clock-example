{
  "name": "fitbit-clock-example",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "devDependencies": {
    "@fitbit/sdk": "~6.0.2",
    "@fitbit/sdk-cli": "^1.7.3",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.1",
    "eslint-plugin-react": "^7.26.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1"
  },
  "fitbit": {
    "appType": "clockface",
    "appDisplayName": "Fitbit Clock Example",
    "iconFile": "resources/icon.png",
    "wipeColor": "#ffffff",
    "requestedPermissions": [
      "access_heart_rate",
      "access_activity"
    ],
    "buildTargets": [
      "atlas",
      "vulcan"
    ],
    "i18n": {},
    "defaultLanguage": "en-US",
    "appUUID": "ae5dfdac-dd77-4d3a-a85b-7a8c2c0fb7e6"
  },
  "scripts": {
    "build": "fitbit-build",
    "debug": "fitbit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
